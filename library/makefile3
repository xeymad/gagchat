C=cc
null_var = "Do null, write \"make compile_server\" or \"make compile_client\""
inc = ../include/
src = /

test_o = test.o
test_c = test.c
dependence_test_h = $(inc)hashtable.h
dependence_test_o = hashtable.o
dependence_test_c = $(src)hashtable.c


null:
	echo 

compile_test: $(test_o) $(dependence_test_o)
	$(C) -c $(test.c)
	$(C) -o test $(test_o) $(dependence_test_o)

compile_libraries: $(test_o) $(dependence_client_o)
	$(C) -c -I $(inc) $(test)
	$(C) -o client $(client_o) $(dependence_client_o)

$(test.o) : $(dependence_client_h)

$(dependence_test_o) : $(dependence_client_h) $(dependence_client_c)
	$(C) -c -I $(inc) $(dependence_client_c)

clean: 
	rm server client *.o


	